@prefix : <http://example.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:IoT_Ontology a owl:Ontology ;
    rdfs:label "IoT_Ontology" .

:address a owl:DatatypeProperty ;
    rdfs:label "address" ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

:deviceId a owl:DatatypeProperty ;
    rdfs:label "deviceId" ;
    rdfs:domain :Device ;
    rdfs:range xsd:string .

:deviceName a owl:DatatypeProperty ;
    rdfs:label "deviceName" ;
    rdfs:domain :Device ;
    rdfs:range xsd:string .

:lastSeen a owl:DatatypeProperty ;
    rdfs:label "lastSeen" ;
    rdfs:domain :Device ;
    rdfs:range xsd:dateTime .

:locatedAt a owl:ObjectProperty ;
    rdfs:label "locatedAt" ;
    rdfs:domain :Device ;
    rdfs:range :Location .

:locationId a owl:DatatypeProperty ;
    rdfs:label "locationId" ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

:locationName a owl:DatatypeProperty ;
    rdfs:label "locationName" ;
    rdfs:domain :Location ;
    rdfs:range xsd:string .

:status a owl:DatatypeProperty ;
    rdfs:label "status" ;
    rdfs:domain :Device ;
    rdfs:range xsd:string .

:temperature a owl:DatatypeProperty ;
    rdfs:label "temperature" ;
    rdfs:domain :Device ;
    rdfs:range xsd:double .

:Location a owl:Class ;
    rdfs:label "Location" .

:Device a owl:Class ;
    rdfs:label "Device" .

