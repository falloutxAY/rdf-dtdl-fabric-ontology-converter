@prefix : <http://example.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Manufacturing_Ontology a owl:Ontology ;
    rdfs:label "Manufacturing_Ontology" .

:attachedTo a owl:ObjectProperty ;
    rdfs:label "attachedTo" ;
    rdfs:domain :Sensor ;
    rdfs:range :Equipment .

:containsEquipment a owl:ObjectProperty ;
    rdfs:label "containsEquipment" ;
    rdfs:domain :Facility ;
    rdfs:range :Equipment .

:equipmentId a owl:DatatypeProperty ;
    rdfs:label "equipmentId" ;
    rdfs:domain :Equipment ;
    rdfs:range xsd:string .

:equipmentName a owl:DatatypeProperty ;
    rdfs:label "equipmentName" ;
    rdfs:domain :Equipment ;
    rdfs:range xsd:string .

:facilityId a owl:DatatypeProperty ;
    rdfs:label "facilityId" ;
    rdfs:domain :Facility ;
    rdfs:range xsd:string .

:facilityName a owl:DatatypeProperty ;
    rdfs:label "facilityName" ;
    rdfs:domain :Facility ;
    rdfs:range xsd:string .

:hasSensor a owl:ObjectProperty ;
    rdfs:label "hasSensor" ;
    rdfs:domain :Equipment ;
    rdfs:range :Sensor .

:installDate a owl:DatatypeProperty ;
    rdfs:label "installDate" ;
    rdfs:domain :Equipment ;
    rdfs:range xsd:dateTime .

:isOperational a owl:DatatypeProperty ;
    rdfs:label "isOperational" ;
    rdfs:domain :Equipment ;
    rdfs:range xsd:boolean .

:locatedIn a owl:ObjectProperty ;
    rdfs:label "locatedIn" ;
    rdfs:domain :Equipment ;
    rdfs:range :Facility .

:location a owl:DatatypeProperty ;
    rdfs:label "location" ;
    rdfs:domain :Facility ;
    rdfs:range xsd:string .

:manufacturer a owl:DatatypeProperty ;
    rdfs:label "manufacturer" ;
    rdfs:domain :Equipment ;
    rdfs:range xsd:string .

:maxValue a owl:DatatypeProperty ;
    rdfs:label "maxValue" ;
    rdfs:domain :Sensor ;
    rdfs:range xsd:double .

:minValue a owl:DatatypeProperty ;
    rdfs:label "minValue" ;
    rdfs:domain :Sensor ;
    rdfs:range xsd:double .

:sensorId a owl:DatatypeProperty ;
    rdfs:label "sensorId" ;
    rdfs:domain :Sensor ;
    rdfs:range xsd:string .

:sensorName a owl:DatatypeProperty ;
    rdfs:label "sensorName" ;
    rdfs:domain :Sensor ;
    rdfs:range xsd:string .

:sensorType a owl:DatatypeProperty ;
    rdfs:label "sensorType" ;
    rdfs:domain :Sensor ;
    rdfs:range xsd:string .

:unit a owl:DatatypeProperty ;
    rdfs:label "unit" ;
    rdfs:domain :Sensor ;
    rdfs:range xsd:string .

:Facility a owl:Class ;
    rdfs:label "Facility" .

:Sensor a owl:Class ;
    rdfs:label "Sensor" .

:Equipment a owl:Class ;
    rdfs:label "Equipment" .

