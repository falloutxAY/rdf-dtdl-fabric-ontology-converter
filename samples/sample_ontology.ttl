@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/ontology/> .

# Sample Ontology for Testing Fabric Ontology Import
# This ontology models a simple manufacturing domain with Equipment and Sensors

# Ontology declaration
ex:ManufacturingOntology rdf:type owl:Ontology ;
    rdfs:label "Manufacturing Ontology" ;
    rdfs:comment "A sample ontology for manufacturing equipment and sensors" .

# =============================================================================
# Classes (Entity Types)
# =============================================================================

# Equipment class
ex:Equipment rdf:type owl:Class ;
    rdfs:label "Equipment" ;
    rdfs:comment "Industrial equipment in a manufacturing facility" .

# Sensor class
ex:Sensor rdf:type owl:Class ;
    rdfs:label "Sensor" ;
    rdfs:comment "Sensors attached to equipment for monitoring" .

# Facility class
ex:Facility rdf:type owl:Class ;
    rdfs:label "Facility" ;
    rdfs:comment "Manufacturing facility or plant" .

# =============================================================================
# Data Properties (Entity Type Properties)
# =============================================================================

# Equipment properties
ex:equipmentId rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Equipment ;
    rdfs:range xsd:string ;
    rdfs:label "Equipment ID" ;
    rdfs:comment "Unique identifier for the equipment" .

ex:equipmentName rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Equipment ;
    rdfs:range xsd:string ;
    rdfs:label "Equipment Name" ;
    rdfs:comment "Display name of the equipment" .

ex:manufacturer rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Equipment ;
    rdfs:range xsd:string ;
    rdfs:label "Manufacturer" ;
    rdfs:comment "Equipment manufacturer" .

ex:installDate rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Equipment ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Installation Date" ;
    rdfs:comment "Date when the equipment was installed" .

ex:isOperational rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Equipment ;
    rdfs:range xsd:boolean ;
    rdfs:label "Is Operational" ;
    rdfs:comment "Whether the equipment is currently operational" .

# Sensor properties
ex:sensorId rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Sensor ;
    rdfs:range xsd:string ;
    rdfs:label "Sensor ID" ;
    rdfs:comment "Unique identifier for the sensor" .

ex:sensorName rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Sensor ;
    rdfs:range xsd:string ;
    rdfs:label "Sensor Name" ;
    rdfs:comment "Display name of the sensor" .

ex:sensorType rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Sensor ;
    rdfs:range xsd:string ;
    rdfs:label "Sensor Type" ;
    rdfs:comment "Type of sensor (temperature, pressure, etc.)" .

ex:unit rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Sensor ;
    rdfs:range xsd:string ;
    rdfs:label "Unit" ;
    rdfs:comment "Measurement unit of the sensor" .

ex:minValue rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Sensor ;
    rdfs:range xsd:double ;
    rdfs:label "Minimum Value" ;
    rdfs:comment "Minimum measurable value" .

ex:maxValue rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Sensor ;
    rdfs:range xsd:double ;
    rdfs:label "Maximum Value" ;
    rdfs:comment "Maximum measurable value" .

# Facility properties
ex:facilityId rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Facility ;
    rdfs:range xsd:string ;
    rdfs:label "Facility ID" ;
    rdfs:comment "Unique identifier for the facility" .

ex:facilityName rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Facility ;
    rdfs:range xsd:string ;
    rdfs:label "Facility Name" ;
    rdfs:comment "Display name of the facility" .

ex:location rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Facility ;
    rdfs:range xsd:string ;
    rdfs:label "Location" ;
    rdfs:comment "Physical location of the facility" .

# =============================================================================
# Object Properties (Relationship Types)
# =============================================================================

# Equipment has Sensor relationship
ex:hasSensor rdf:type owl:ObjectProperty ;
    rdfs:domain ex:Equipment ;
    rdfs:range ex:Sensor ;
    rdfs:label "Has Sensor" ;
    rdfs:comment "Equipment has an attached sensor" .

# Facility contains Equipment relationship
ex:containsEquipment rdf:type owl:ObjectProperty ;
    rdfs:domain ex:Facility ;
    rdfs:range ex:Equipment ;
    rdfs:label "Contains Equipment" ;
    rdfs:comment "Facility contains equipment" .

# Inverse relationship: Sensor attached to Equipment
ex:attachedTo rdf:type owl:ObjectProperty ;
    owl:inverseOf ex:hasSensor ;
    rdfs:domain ex:Sensor ;
    rdfs:range ex:Equipment ;
    rdfs:label "Attached To" ;
    rdfs:comment "Sensor is attached to equipment" .

# Inverse relationship: Equipment located in Facility
ex:locatedIn rdf:type owl:ObjectProperty ;
    owl:inverseOf ex:containsEquipment ;
    rdfs:domain ex:Equipment ;
    rdfs:range ex:Facility ;
    rdfs:label "Located In" ;
    rdfs:comment "Equipment is located in facility" .
